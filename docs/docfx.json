{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "/**/*.{md,yml}",
          "toc.yml",
          "docs/**.md",
          "docs/**.yml",
          "*.md",
          "docs/*.md",
          "api/**.yml",
          "api/index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "Quickie",
      "_appTitle": "Quickie",
      "_googleAnalyticsTagId": "G-S4FG36P7BW",
      "_enableSearch": true,
      "pdf": true
    }
  }
}